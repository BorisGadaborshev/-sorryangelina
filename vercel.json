{
  "version": 2,
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 